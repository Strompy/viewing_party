<h1>40 Top Rated Movies of All Time</h1>


<div class="blockbusters">
<ol>
  <% @movies.each_with_index do |movie| %>
    <section class="movie">
      <li class="title">Title: <%= link_to movie.title, "/movies/#{movie.id}" %> Vote Average: <%= movie.vote_average %></li>
    </section>
  <% end %>
</ol>
</div>
